{
  "name": "akaramires/m3u-parser",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Akaramires\\M3uParser\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Akaramires\\M3uParser\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "role": "Developer",
      "name": "Elmar Abdurayimov",
      "email": "e.abdurayimov@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "spatie/laravel-data": "4.5"
  },
  "require-dev": {
    "phpunit/phpunit": "10.*"
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Akaramires\\M3uParser\\Providers\\M3uParserServiceProvider"
      ]
    }
  }
}
